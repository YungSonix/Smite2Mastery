{
    "buildCommand": "npx expo export --platform web",
    "outputDirectory": "dist",
    "devCommand": "npx expo start --web",
    "framework": null,
    "rewrites": [
      {
        "source": "/((?!robots\\.txt|sitemap\\.xml|favicon\\.ico|.*\\.(js|css|png|jpg|jpeg|gif|svg|webp|json)).*)",
        "destination": "/index.html"
      }
    ]
  }